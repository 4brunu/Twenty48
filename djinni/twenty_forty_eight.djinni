move = enum {
  up;
  down;
  left;
  right;
}

player = interface +c {
  static create(): player;
  new_game();
  const score(): i64;
  const has_won(): bool;
  const game_state(): list<i16>;
  const game_over(): bool;
  # @param move the direction of movement
  # @return bool false if the move was unsuccessful
  swipe(move: move): bool;
  const moves_made(): i64;
}
