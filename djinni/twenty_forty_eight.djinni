move = enum {
  up;
  down;
  left;
  right;
}

player = interface +c {
  static create(): player;
  new_game();
  score(): i64;
  has_won(): bool;
  game_state(): list<i32>;
  game_over(): bool;
  # @param move the direction of movement
  # @return bool false if the move was unsuccessful
  swipe(move: move): bool;
}
